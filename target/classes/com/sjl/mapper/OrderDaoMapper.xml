<mapper namespace="com.sjl.dao.OrderDao">

    <insert id="createOrder" parameterType="order" useGeneratedKeys="true" keyProperty="id">
        insert into stock_order values (#{id},#{sid},#{name},#{createDate})
    </insert>

</mapper>